#!/bin/bash
# Test OKX CDN availability across years

for year in 2021 2022 2023 2024; do
  for month in 01 06 12; do
    url="https://www.okx.com/cdn/okex/traderecords/swaprate/daily/${year}${month}01/BTC-USDT-SWAP-swaprate-${year}-${month}-01.zip"
    http_code=$(curl -s -o /dev/null -w "%{http_code}" "$url")
    echo "${year}-${month}-01: HTTP $http_code"
  done
done
